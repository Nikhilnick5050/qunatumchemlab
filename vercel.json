{
  "version": 2,
  "functions": {
    "backend/api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/backend/api/index.js"
    },
    {
      "src": "^/$",
      "dest": "/public/index.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/public/$1"
    }
  ]
}